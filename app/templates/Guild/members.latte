<!DOCTYPE HTML>
<html>
<head>
  {include "../head.latte"}
  <title>HeroesofAbenez {$server} - Guild members</title>
</head>
<body>
{include "../navigation.latte"}
<article id='main'>
  <h1>Guild members</h1>
  {foreach $members as $member}{$member["rank"]} {$member["name"]}{if $canPromote AND $member["rankId"] < $rankId} <a n:href="Guild:promote $member['id']">Promote</a> <a n:href="Guild:demote $member['id']">Demote</a>{/if}{if $canKick AND $member["rankId"] < $rankId} <a n:href="Guild:kick $member['id']">Kick</a>{/if}{if !$iterator->isLast()}<br>{/if}{/foreach}
</article>
{include "../footer.latte"}
</body>
</html>