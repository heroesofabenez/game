{layout '../@layout.latte'}
{block title}Guild {$name}{/block}
{block content}
  <h1>Guild {$name}</h1>
  <p>Description: {$description}</p>
  <h2>Members</h2>
  {foreach $members as $member}
    {$member["rank"]}: <a n:href="Profile:view $member['id']">{$member["name"]}</a><br>
  {/foreach}
{/block}