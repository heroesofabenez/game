includes:
    - local.neon
application:
    mapping:
        *: HeroesofAbenez\Presenters\*Presenter
    scanDirs: no
tracy:
    strictMode: TRUE
session:
    savePath: "%tempDir%/sessions"
    name: HOASESSID
    expiration: 1 hour
extensions:
    translation: Kdyby\Translation\DI\TranslationExtension
    hoa: HeroesofAbenez\Model\DI\HOAExtension
translation:
    default: en
    fallback: [en_US, en]
    resolvers:
        request: off
