{
    "name": "heroesofabenez/game",
    "description": "Work in progress online game Heroes of Abenez. Written in PHP 7, built on Nette Framework.",
    "homepage": "http://heroesofabenez.tk/",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "lib-curl": "*",
        "ext-spl": "*",
        "nette/application": "~2.4",
        "nette/bootstrap": "~2.4",
        "nette/caching": "~2.5",
        "nette/database": "~2.4",
        "nette/di": "~2.4",
        "nette/forms": "~2.4",
        "nette/security": "~2.4",
        "nette/utils": "~2.4",
        "tracy/tracy": "~2.4",
        "latte/latte": "~2.4",
        "kdyby/autowired": "^1.4",
        "kdyby/translation": "2.3.x@dev"
    },
    "require-dev": {
        "konecnyjakub/mytester": "2.0.x@dev",
        "nette/robot-loader": "~2.4",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "squizlabs/php_codesniffer": "^2.8",
        "kdyby/fake-session": "^2.0",
        "phpstan/phpstan": "^0.6.4",
        "phpstan/phpstan-nette": "^0.6.3"
    },
    "autoload": {
        "psr-4": {
            "HeroesofAbenez\\": [
                "app/"
            ]
        },
        "classmap": ["app/Model/exceptions.php"]
    },
    "config": {
        "classmap-authoritative": true
    }
}
